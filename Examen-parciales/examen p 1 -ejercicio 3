//hacer una funcion que reciba como parametro un vector ya cargado y la cantidad de elementos del vector
//la funcion debera retornar ://
//n=10
//v= 18  13   12  34  5   11  9  2  4  7
//--->v= 18  2  12  5  13  11  9  34  4  7

#include <iostream>
#include <conio.h>

using namespace std;

void cargar(int v[],int n){
	int i,x;
    for(i=0;i<n;i++){
		cout<<"elemento :";
		cin>>x;
		v[i]=x;
	}
}
void mostrar(int v[],int n){
	int i;
	for(i=0;i<n;i++){
		cout<<v[i]<<" ";
	}
}
void ordenburbuja(int v[],int n){
int i=0,j,aux;
for(i=0;i<n;i++){
	 for(j=i+1;j<n;j++){
		 if(v[i]>v[j]){
			 aux=v[i];
			 v[i]=v[j];
	         v[j]=aux;
		 }
	 }
 }
}

bool fibonacci(int n){
	bool k=false;
	int a,b,i,c;
	a=1;
	b=1;
	for(i=0;i<=n;i++){
		if(n==b){
			k=true;
		}
		c=a+b;
		a=b;
		b=c;
		
	}
	return(k);
}

void ordenar(int v[],int n){
	int i,vec[100],k=0,j;
	for(i=0;i<n;i++){
		if(fibonacci(v[i])==true){
			vec[k]=v[i];
			k++;
			v[i]=0;
		}

	}
	
	ordenburbuja(vec,k);
	k=0;
	for(j=0;j<n;j++){
		if(v[j]==0){
			v[j]=vec[k];
			k++;
		}
     }
	cout<<"el vector resultante es:"<<endl;
	mostrar(v,n);
}



void main(){
	int n,v[100];
	cout<<"ingrese tamaÃ±o del vector:";
	cin>>n;
	cargar(v,n);
	ordenar(v,n);
    getch();
}

