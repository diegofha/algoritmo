using namespace std;

bool esprimo(int n){
	int i;
	bool k=true;
	for(i=2;i<n;i++){
		if(n%2==0){
			k=false;
		}
	}
	return (k);
}

int cantidad1(int primo){
	 for(i=1;c=1;i++){
		if(esprimo(primo)==true){
			
			c=c+1;
		    }
		primo=primo+1;
	     }
	   return (c);
}

