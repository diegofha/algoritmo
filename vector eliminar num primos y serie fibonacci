#include <iostream>
#include <conio.h>

using namespace std;

void cargar(int v[],int n){
	int i,x;
    for(i=0;i<n;i++){
		cout<<"elemento :";
		cin>>x;
		v[i]=x;
	}
}
void mostrar(int v[],int n){
	int i;
	for(i=0;i<n;i++){
		cout<<v[i]<<" ";
	}
}
bool esprimo(int n){
bool k=true;
int i;
for(i=2;i<n;i++){
	if(n%i==0){
		k=false;
	}
 }
return (k);
}
void ordenburbuja(int v[],int n){
int i=0,j,aux;
for(i=0;i<n;i++){
	 for(j=i+1;j<n;j++){
		 if(v[i]>v[j]){
			 aux=v[i];
			 v[i]=v[j];
	         v[j]=aux;
		 }
	 }
 }
}

bool fibonacci(int n){
	bool k=false;
	int a,b,i,c;
	a=1;
	b=1;
	for(i=0;i<=n;i++){
		if(n==b){
			k=true;
		}
		c=a+b;
		a=b;
		b=c;
		
	}
	return(k);
}

void ordenar(int v[],int n){
	int i,vec[100],k=0,j;
	for(i=0;i<n;i++){
		if(fibonacci(v[i])==false || esprimo(v[i])==false){
			vec[k]=v[i];
			k++;
		}	
	}
	cout<<"el vector resultante es:"<<endl;
	mostrar(vec,k);	

}



void main(){
	int n,v[100];
	cout<<"ingrese tamaÃ±o del vector:";
	cin>>n;
	cargar(v,n);
	ordenar(v,n);
    getch();
}

