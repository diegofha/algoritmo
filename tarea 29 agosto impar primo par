#include <iostream>
#include <conio.h>

using namespace std;

bool esprimo(int n){
	int i;
	bool k=true;
	for(i=2;i<n;i++){
		if(n%2==0){
			k=false;
		}
	}
	return (k);
}

int cantidad1(int primo){
	 for(i=1;c=1;i++){
		if(esprimo(primo)==true){
			
			c=c+1;
		    }
		primo=primo+1;
	     }
	   return (c);
}

void main(){
	int impar=1,primo=1,par=2,turno=1,n,c=0;
		cout<<"ingrese el numero de la secuencia:";
	    cin>>n;
		while(c<n){
			
			if(turno==1){
				cout<<impar;
				impar=impar+2;
			}
			else{
				if(turno==2){
			     for(i=1;cantidad1(primo)=2;i++){
						 cout<<primo;
					primo=primo+1;
				 }
				}
				else(turno==3){
					cout<<par;
                 par=par+2;
				}
			}
			c++;
