int  elementomenor(int v[],int n){
	int i,menor=99999;
	for(i=0;i<n;i++){
		if(v[i]<menor){
			menor=v[i];
		}
	}return(menor);
}

int menordetodos(int a,int b){
	int menor;
	if(a<b){
		menor=a;
	}else{
		menor=b;
	}
	return(menor);
}

void eliminarcasilla(int v[],int &n,int x){
	int i,k;
	x=elementomenor(v,n);
	for(i=0;i<n;i++){
		if(v[i]==x){
			for(k=i;k<n-1;k++){
				v[k]=v[k+1];
			}n--;
			i--;
		}
	}
}



