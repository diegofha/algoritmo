#include <iostream>
#include <conio.h>
using namespace std;

void funcion(int n){
	int i,j;
	for(i=1;i<n;i++){
		cout<<i<<"\n";
		for(j=i+1;j<=i+1;j++){
			cout<<j<<" ";
		}
	}
}





void main(){
	int n;
	cin>>n;
	funcion(n);
	getch();
}














