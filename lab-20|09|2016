#include <iostream>
#include <conio.h>

using namespace std;

struct vector{
	int v[100];
	int nsv;
}

void cargar(vector vec[],int n){
	int i,k;
	for(i=0;i<n;i++){
		cout<<"numero de elementos:";
		cin>>v[i].nsv;
		for(k=0;k<n;k++){
			cout<<"numero:";
			cin>>vec[i].v[k];
		}
	}
}



void mostrar(vector vec[],int n){
	int i,k;
	for(i=0;i<n;i++){
		for(k=0;k<n;k++){
			cout<<vec[i].v[k];
		}
	}
}

void funcion(vector vec[],int n){
	int i,j,dp=1,t1=1,ts=1;
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			if(i==j){
				vec[i].v[j]=dp++;
			}else{
				if(i<j){
					vec[i].v[j]=ti;
					ti=2;
				}
			}
		}
	}
}
void ordenar(int vec[],int n){
	int i,j,aux;
	for(i=0;i<n;i++){
		for(j=i;j<n;j++){
			if(vec[i] >vec[j]){
				aux=vec[i];
				vec[i]=vec[j];
				vec[j]=aux;
			}
		}
	}
}

//ordenar aascendentemente lso subvectore
 void ordenar(vector vec[],int n){
 int i,
      for(i=0;i<n;i++){
       ordenar(vec[i].v,n);
	  }
 }

 bool primo(int n){
	 bool k=true;
	 int i;
	 for(i=2;i<n;i++){
		 if(n%i==0){
			 k=false;
		 }
	 }
	 return (k);
 }

void elementosprimos(vector vec[],int n){
	int vx[100],i,k,j=0;

	for(i=0;i<n;i++){
		for(k=0;k<n;k++){
		    if(primo(vec[i].v[k])==true){
				vx[j++]=vec[i].v[k];
			}
		}
	}
}
